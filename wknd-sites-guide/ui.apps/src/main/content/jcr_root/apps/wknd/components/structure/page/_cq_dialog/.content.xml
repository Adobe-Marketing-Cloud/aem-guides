<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.page.properties,cq.wcm.msm.properties]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v1"
    mode="edit">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <pageicon     
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Page Icon"
                                        sling:orderBefore="onofftime"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <icon
                                            	cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="Select"
                                                fieldLabel="Icon"
                                                name="./pageIcon"
                                                renderReadOnly="{Boolean}true"
                                                translateOptions="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="pageIcon"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <art
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Art"
                                                        value="wkndicon-ico-art"/>
                                                    <drink
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Drink"
                                                        value="wkndicon-ico-bars"/>
                                                    <music
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Music"
                                                        value="wkndicon-ico-music"/>
                                                    <restaurant
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Restaurant"
                                                        value="wkndicon-ico-restaurants"/>
                                                    <shopping
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Shopping"
                                                        value="wkndicon-ico-shopping"/>
                                                    <sport
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Sport"
                                                        value="wkndicon-ico-sport"/>
                                                </items>
                                            </icon>
                                        </items>
                                    </pageicon>
                                </items>
                            </column>
                        </items>
                    </basic>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
